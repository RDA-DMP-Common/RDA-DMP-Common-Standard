{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://github.com/RDA-DMP-Common/RDA-DMP-Common-Standard/tree/master/examples/JSON/JSON-schema/1.2",
  "title": "RDA DMP Common Standard Schema",
  "description": "JSON Schema for the RDA DMP Common Standard",
  "type": "object",
  "properties": {
    "dmp": {
      "$ref": "#/$defs/DMPData"
    }
  },
  "required": ["dmp"],
  "$defs": {
    "Booleanish": {
      "$anchor": "Booleanish",
      "type": "string",
      "enum": [
        "yes",
        "no",
        "unknown"
      ],
      "title": "The DMP Ethical Issues Exist Schema",
      "description": "To indicate whether there are ethical issues related to data that this DMP describes. Allowed values: yes, no, unknown",
      "examples": [
        "yes"
      ]
    },
    "Certification": {
      "$anchor": "Certification",
      "type": "string",
      "enum": [
        "din31644",
        "dini-zertifikat",
        "dsa",
        "iso16363",
        "iso16919",
        "trac",
        "wds",
        "coretrustseal"
      ],
      "title": "The Dataset Distribution Host Certification Type Schema",
      "description": "Repository certified to a recognised standard. Allowed values: din31644, dini-zertifikat, dsa, iso16363, iso16919, trac, wds, coretrustseal",
      "examples": [
        "coretrustseal"
      ]
    },
    "Contact": {
      "$anchor": "Contact",
      "type": "object",
      "title": "The DMP Contact Schema",
      "properties": {
        "contact_id": {
          "$ref": "#/$defs/ContactID"
        },
        "mbox": {
          "type": "string",
          "format": "email",
          "title": "The Mailbox Schema",
          "description": "Contact Person's E-mail address",
          "examples": [
            "cc@example.com"
          ]
        },
        "name": {
          "type": "string",
          "title": "The Name Schema",
          "description": "Name of the contact person",
          "examples": [
            "Charlie Chaplin"
          ]
        }
      },
      "required": [
        "contact_id",
        "mbox",
        "name"
      ]
    },
    "Contributor": {
      "$anchor": "Contributor",
      "type": "object",
      "title": "The Contributor Items Schema",
      "properties": {
        "contributor_id": {
          "$ref": "#/$defs/ContributorID"
        },
        "mbox": {
          "type": "string",
          "title": "The Contributor Mailbox Schema",
          "description": "Contributor Mail address",
          "examples": [
            "john@smith.com"
          ],
          "format": "email"
        },
        "name": {
          "type": "string",
          "title": "The Name Schema",
          "description": "Name of the contributor",
          "examples": [
            "John Smith"
          ]
        },
        "role": {
          "$ref": "#/$defs/ContributorRoles"
        }
      },
      "required": [
        "contributor_id",
        "name",
        "role"
      ]
    },
    "ContributorID": {
      "$anchor": "ContributorID",
      "type": "object",
      "title": "The Contributor ID Schema",
      "properties": {
        "identifier": {
          "$ref": "#/$defs/ContributorIDValue"
        },
        "type": {
          "$ref": "#/$defs/ContributorIDType"
        }
      },
      "required": [
        "identifier",
        "type"
      ]
    },
    "ContributorIDType": {
      "$anchor": "ContributorIDType",
      "type": "string",
      "enum": [
        "orcid",
        "isni",
        "openid",
        "other"
      ],
      "title": "The DMP Contact Identifier Type Schema",
      "description": "Identifier type. Allowed values: orcid, isni, openid, other",
      "examples": [
        "orcid"
      ]
    },
    "ContributorIDValue": {
      "$anchor": "ContributorIDValue",
      "type": "string",
      "title": "The DMP Contributor Identifier Schema",
      "examples": [
        "https://orcid.org/0000-0003-0644-4174"
      ]
    },
    "ContributorRole": {
      "$anchor": "ContributorRole",
      "type": "string",
      "title": "The Contributor Role(s) Items Schema",
      "examples": [
        "Data Steward"
      ]
    },
    "ContributorRoles": {
      "$anchor": "ContributorRoles",
      "type": "array",
      "title": "The Role Schema",
      "description": "Type of contributor",
      "items": {
        "$ref": "#/$defs/ContributorRole"
      },
      "uniqueItems": true
    },
    "Contributors": {
      "$anchor": "Contributors",
      "type": "array",
      "title": "The Contributor Schema",
      "items": {
        "$ref": "#/$defs/Contributor"
      }
    },
    "ContactID": {
      "$anchor": "ContactID",
      "type": "object",
      "title": "The Contact ID Schema",
      "properties": {
        "identifier": {
          "$ref": "#/$defs/ContactIDValue"
        },
        "type": {
          "$ref": "#/$defs/ContactIDType"
        }
      },
      "required": [
        "identifier",
        "type"
      ]
    },
    "ContactIDType": {
      "$anchor": "ContactIDType",
      "type": "string",
      "enum": [
        "orcid",
        "isni",
        "openid",
        "other"
      ],
      "title": "The DMP Contact Identifier Type Schema",
      "description": "Identifier type. Allowed values: orcid, isni, openid, other",
      "examples": [
        "orcid"
      ]
    },
    "ContactIDValue": {
      "$anchor": "ContactIDValue",
      "type": "string",
      "title": "The DMP Contact Identifier Schema",
      "examples": [
        "https://orcid.org/0000-0003-0644-4174"
      ]
    },
    "Cost": {
      "$anchor": "Cost",
      "type": "object",
      "title": "The Cost Items Schema",
      "properties": {
        "currency_code": {
          "$ref": "#/$defs/CurrencyCode"
        },
        "description": {
          "type": "string",
          "title": "The Cost Description Schema",
          "description": "Cost(s) Description",
          "examples": [
            "Costs for maintaining..."
          ]
        },
        "title": {
          "type": "string",
          "title": "The Cost Title Schema",
          "description": "Title",
          "examples": [
            "Storage and Backup"
          ]
        },
        "value": {
          "type": "number",
          "title": "The Cost Value Schema",
          "description": "Value",
          "examples": [
            1000
          ]
        }
      },
      "required": [
        "title"
      ]
    },
    "Costs": {
      "$anchor": "Costs",
      "type": "array",
      "title": "The Cost Schema",
      "items": {
        "$ref": "#/$defs/Cost"
      }
    },
